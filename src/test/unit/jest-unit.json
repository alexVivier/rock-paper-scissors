{
  "rootDir": "../..",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/$1"
  },
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["./api/**/*.service.ts", "./auth/**/*.service.ts"],
  "coveragePathIgnorePatterns": [
  ],
  "testEnvironment": "node"
}
